{
  "name": "agentql-mcp-server",
  "version": "0.0.1",
  "description": "Model Context Protocol server to work with AgentQL",
  "license": "MIT",
  "repository": "https://github.com/tinyfish-io/mcp-agentql.git",
  "type": "module",
  "bin": {
    "agentql": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.6.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/node-fetch": "^2.6.12",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "mcp",
    "agentql",
    "data-extraction",
    "web-scraping",
    "content-extraction"
  ]
}
